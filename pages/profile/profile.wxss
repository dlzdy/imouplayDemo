/* pages/profile/childCpns/w-header/w-header.wxss */
.header {
  position: relative;
  display: flex;
  background: #f18d00;
  height: 120rpx;
  padding: 120rpx 30rpx;
  color: #fff;
}

.avatar image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 70rpx;
  background: #f2f2f2;
  vertical-align: middle;
}

.login-info {
  vertical-align: middle;
  margin-left: 30rpx;
}

.login-info .login {
  font-size: 36rpx;
  margin-top: 36rpx;
}



